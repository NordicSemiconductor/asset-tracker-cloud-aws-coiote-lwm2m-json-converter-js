{
  "name": "@nordicsemiconductor/coiote-aws-converter-js",
  "description": "Convert the LwM2M JSON encoding written by AVSystem's Coiote AWS integration to nRF Asset Tracker's LwM2M JSON encoding.",
  "scripts": {
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "prepublish": "npx swc -d dist ./src"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-coiote-aws-converter-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-coiote-aws-converter-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-coiote-aws-converter-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "lwm2m",
    "coiote"
  ],
  "dependencies": {
    "@nordicsemiconductor/lwm2m-types": "2.6.1",
    "ajv": "8.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "18.4.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.104",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.23",
    "@swc/cli": "0.1.62",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "tsx": "4.0.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "export.js",
    "index.d.ts",
    "example.ts",
    "dist",
    "src",
    "package-lock.json",
    "LICENSE",
    "README.md"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause"
}
